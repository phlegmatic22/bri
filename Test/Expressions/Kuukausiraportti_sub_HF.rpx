  <Script><![CDATA[static int i = 0;

public void Detail_Format()
{

	// Use the modulo operator to achieve alternating back color 
	// for each row
	if (i % 2 == 0)
	{
	Shape sh = (Shape)rpt.Sections["Detail"].Controls["detailShape"];
	sh.BackColor = System.Drawing.Color.FromArgb(247,247,247); 
	}
	else 
	{
	Shape sh = (Shape)rpt.Sections["Detail"].Controls["detailShape"];
	sh.BackColor = System.Drawing.Color.White; 
	}
	i++; 
	
	
	DataDynamics.ActiveReports.TextBox RS_vrk = (DataDynamics.ActiveReports.TextBox)rpt.Sections["Detail"].Controls["RS_vrk"];
	DataDynamics.ActiveReports.TextBox HF_pitoisuus_vrk = (DataDynamics.ActiveReports.TextBox)rpt.Sections["Detail"].Controls["HF_pitoisuus_vrk"];
	DataDynamics.ActiveReports.TextBox HF_kelpoisuus = (DataDynamics.ActiveReports.TextBox)rpt.Sections["Detail"].Controls["HF_kelpoisuus"];
	DataDynamics.ActiveReports.TextBox HF_status = (DataDynamics.ActiveReports.TextBox)rpt.Sections["Detail"].Controls["HF_status"];

	// Kelpoisuus
	// WI
	if (System.Convert.ToSingle(RS_vrk.Text) < 1.0)
	{	
		HF_kelpoisuus.Text = "Seis";	
	}
	else if(HF_status.Text == "Bad")
	{
		HF_kelpoisuus.Text = "Epäkelpo";
		//Hylättyjen laskuri
		//counter = counter + 1; 
	} 	
	else if(HF_status.Text == "Good")
	{		 
		HF_kelpoisuus.Text = "Kelpo";
	}	
	else
	{
		HF_kelpoisuus.Text = "";
	}
	
	// Pitoisuusarvojen tarkastus
	// WI
	if (HF_pitoisuus_vrk.Text != string.Empty && System.Convert.ToSingle(HF_pitoisuus_vrk.Text) > System.Convert.ToSingle(g_Parameters["LIMIT_HF_WI"]))
	{
		HF_pitoisuus_vrk.ForeColor = System.Drawing.Color.Red;
	}
	else if (HF_pitoisuus_vrk.Text != string.Empty && System.Convert.ToSingle(HF_pitoisuus_vrk.Text) < 0.0)
	{
		HF_pitoisuus_vrk.ForeColor = System.Drawing.Color.Green;
		HF_pitoisuus_vrk.Text = "0,00";
	}
	else
	{
		HF_pitoisuus_vrk.ForeColor = System.Drawing.Color.Black;
	}

}
	
	
public void ActiveReport_DataInitialize()
{
	// Lasketun kentän lisääminen 

  System.Data.DataSet ds = (System.Data.DataSet)rpt.DataSource;   
  ds.Tables[0].Columns.Add("norm2",typeof(double) ,"value2/1000");
  ds.Tables[0].Columns.Add("ylos2",typeof(double) ,"value3/1000");
  ds.Tables[0].Columns.Add("mitta2",typeof(double),"value4/1000"); 
  ds.Tables[0].Columns.Add("kok2",typeof(double),"IIF(value4>0,(value2+value3)/1000,(value2+value3+value4)/1000)");
   	  
  rpt.DataSource = null;
  rpt.DataSource = ds;  

}

public void GroupFooter1_Format()
{
	
	DataDynamics.ActiveReports.TextBox HF_kk_ka = (DataDynamics.ActiveReports.TextBox)rpt.Sections["GroupFooter1"].Controls["HF_kk_ka"];
	
	if (System.Convert.ToSingle(HF_kk_ka.Text) < 0.0)
	{
		HF_kk_ka.ForeColor = System.Drawing.Color.Green;
		HF_kk_ka.Text = "0.00";
	}	
	if (System.Convert.ToSingle(HF_kk_ka.Text) > System.Convert.ToSingle(g_Parameters["LIMIT_HF_WI"]))
	{
		HF_kk_ka.ForeColor = System.Drawing.Color.Red;
	}
	else
	{
		HF_kk_ka.ForeColor = System.Drawing.Color.Black;
	}



	DataDynamics.ActiveReports.TextBox HF_min = (DataDynamics.ActiveReports.TextBox)rpt.Sections["GroupFooter1"].Controls["HF_min"];
	DataDynamics.ActiveReports.TextBox HF_max = (DataDynamics.ActiveReports.TextBox)rpt.Sections["GroupFooter1"].Controls["HF_max"];

	if (System.Convert.ToSingle(HF_min.Text) < 0.0)
	{
		HF_min.Text = "0,00";
	}
	if (System.Convert.ToSingle(HF_max.Text) < 0.0)
	{
		HF_max.Text = "0,00";
	}
	



}
